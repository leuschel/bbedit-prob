		 <string>0123456789ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz_</string>
		 
		 <key>Open Block Comments</key>
		 <string>/*</string>
		 <key>Close Block Comments</key>
		 <string>*/</string>
